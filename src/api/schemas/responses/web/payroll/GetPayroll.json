{
  "type": "object",
  "properties": {
    "thisMonth": {
      "type": "object",
      "required": ["wageInfo", "reservations", "coaching", "userCount"],
      "properties": {
        "userCount": {
          "type": "object",
          "required": ["preUser", "paidUser", "leaveUser"],
          "properties": {
            "preUser": {
              "type": "integer"
            },
            "paidUser": {
              "type": "integer"
            },
            "leaveUser": {
              "type": "integer"
            }
          }
        },
        "wageInfo": {
          "type": "object",
          "required": ["baseWage", "ptPercentage", "fcPercentage"],
          "properties": {
            "baseWage": {
              "type": "integer"
            },
            "ptPercentage": {
              "type": "integer"
            },
            "fcPercentage": {
              "type": "integer"
            }
          }
        },
        "reservations": {
          "type": "array",
          "items": {
            "type": "object",
            "required": [
              "ticketId",
              "type",
              "nickname",
              "sessionPrice",
              "coachingPrice",
              "totalSession",
              "leftSession"
            ],
            "properties": {
              "ticketId": {
                "type": "integer"
              },
              "type": {
                "type": "string"
              },
              "nickname": {
                "type": "string"
              },
              "sessionPrice": {
                "type": "integer"
              },
              "coachingPrice": {
                "type": "integer"
              },
              "totalSession": {
                "type": "integer"
              },
              "leftSession": {
                "type": "integer"
              }
            }
          }
        },
        "coaching": {
          "type": "array",
          "items": {
            "type": "object",
            "required": ["ticketId", "nickname", "type", "startedAt", "expiredAt", "holdingList"],
            "properties": {
              "ticketId": {
                "type": "integer"
              },
              "nickname": {
                "type": "string"
              },
              "type": {
                "type": "integer"
              },
              "startedAt": {
                "type": "string"
              },
              "expiredAt": {
                "type": "string"
              },
              "holdingList": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "holdId": {
                      "type": "integer"
                    },
                    "startAt": {
                      "type": "string"
                    },
                    "endAt": {
                      "type": "string"
                    },
                    "days": {
                      "type": "integer"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
