{
  "type": "object",
  "properties": {
    "workoutScheduleId": {
      "type": "integer"
    },
    "startDate": {
      "type": "string"
    },
    "workoutTitle": {
      "type": "string"
    },
    "workoutSubTitle": {
      "type": "string"
    },
    "targetMuscleTypes": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "workoutTotalTime": {
      "type": "string",
      "example": "00:00:00"
    },
    "isWorkoutComplete": {
      "type": "boolean"
    },
    "strengthIndex": {
      "type": "integer",
      "minimum": 1,
      "maximum": 5
    },
    "issueIndexes": {
      "type": "array",
      "items": {
        "type": "integer",
        "minimum": 1,
        "maximum": 5
      },
      "nullable": true
    },
    "contents": {
      "type": "string",
      "minLength": 1,
      "maxLength": 500,
      "nullable": true
    },
    "trainerNickname": {
      "type": "string"
    },
    "trainerProfileImage": {
      "type": "string"
    },
    "exercises": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "workoutPlanId": {
            "type": "integer"
          },
          "name": {
            "type": "string"
          },
          "description": {
            "type": "string"
          },
          "trackingFieldId": {
            "type": "integer"
          },
          "targetMuscles": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "name": {
                  "type": "string"
                },
                "type": {
                  "type": "string",
                  "enum": ["main", "sub"]
                }
              }
            }
          },
          "videos": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "url": {
                  "type": "string"
                },
                "index": {
                  "type": "integer"
                },
                "thumbnail": {
                  "type": "string"
                }
              }
            }
          },
          "setInfo": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "index": {
                  "type": "integer",
                  "minimum": 1,
                  "maximum": 15
                },
                "reps": {
                  "type": "integer",
                  "minimum": 1,
                  "maximum": 100
                },
                "weight": {
                  "type": "integer",
                  "minimum": 1,
                  "maximum": 500
                },
                "seconds": {
                  "type": "integer",
                  "minimum": 1,
                  "maximum": 10800
                }
              }
            }
          },
          "recordSetInfo": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "index": {
                  "type": "integer",
                  "minimum": 1,
                  "maximum": 15
                },
                "reps": {
                  "type": "integer",
                  "minimum": 1,
                  "maximum": 100
                },
                "weight": {
                  "type": "integer",
                  "minimum": 1,
                  "maximum": 500
                },
                "seconds": {
                  "type": "integer",
                  "minimum": 1,
                  "maximum": 10800
                }
              }
            },
            "nullable": true
          }
        },
        "required": [
          "workoutPlanId",
          "name",
          "description",
          "trackingFieldId",
          "targetMuscles",
          "videos",
          "setInfo",
          "recordSetInfo"
        ]
      }
    }
  },
  "required": [
    "workoutScheduleId",
    "workoutTitle",
    "workoutSubTitle",
    "targetMuscleTypes",
    "workoutTotalTime",
    "exercises"
  ]
}
