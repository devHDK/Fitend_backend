{
  "type": "object",
  "properties": {
    "startDate": {
      "type": "string"
    },
    "strengthIndex": {
      "type": "integer",
      "minimum": 1,
      "maximum": 5
    },
    "issueIndexes": {
      "type": "array",
      "items": {
        "type": "integer",
        "minimum": 1,
        "maximum": 5
      },
      "nullable": true
    },
    "contents": {
      "type": "string",
      "minLength": 1,
      "maxLength": 500,
      "nullable": true
    },
    "workoutRecords": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "exerciseName": {
            "type": "string"
          },
          "targetMuscles": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "trackingFieldId": {
            "type": "integer"
          },
          "workoutPlanId": {
            "type": "integer"
          },
          "setInfo": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "index": {
                  "type": "integer",
                  "minimum": 1,
                  "maximum": 15
                },
                "reps": {
                  "type": "integer",
                  "minimum": 1,
                  "maximum": 99,
                  "nullable": true
                },
                "weight": {
                  "type": "number",
                  "minimum": 0.1,
                  "maximum": 999,
                  "nullable": true
                },
                "seconds": {
                  "type": "integer",
                  "minimum": 0,
                  "maximum": 10800,
                  "nullable": true
                }
              }
            }
          }
        }
      }
    },
    "scheduleRecords": {
      "type": "object",
      "nullable": true,
      "properties": {
        "heartRates": {
          "type": "array",
          "nullable": true,
          "items": {
            "type": "integer"
          }
        },
        "workoutDuration": {
          "type": "integer",
          "nullable": true
        },
        "calories": {
          "type": "integer",
          "nullable": true
        }
      }
    },
    "schedules": {
      "type": "object",
      "properties": {
        "data": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "startDate": {
                "type": "string",
                "format": "date"
              },
              "workouts": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "workoutScheduleId": {
                      "type": "integer"
                    },
                    "seq": {
                      "type": "integer"
                    },
                    "title": {
                      "type": "string"
                    },
                    "subTitle": {
                      "type": "string"
                    },
                    "isComplete": {
                      "type": "boolean"
                    },
                    "isRecord": {
                      "type": "boolean"
                    }
                  },
                  "required": ["workoutScheduleId", "seq", "title", "subTitle", "isComplete", "isRecord"]
                }
              }
            },
            "required": ["startDate", "workouts"],
            "additionalProperties": false
          }
        }
      },
      "required": ["data"],
      "additionalProperties": false
    },
    "additionalProperties": false
  }
}
