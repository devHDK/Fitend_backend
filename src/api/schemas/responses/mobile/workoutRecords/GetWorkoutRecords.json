{
  "type": "object",
  "properties": {
    "startDate": {
      "type": "string"
    },
    "strengthIndex": {
      "type": "integer",
      "minimum": 1,
      "maximum": 5
    },
    "issueIndexes": {
      "type": "array",
      "items": {
        "type": "integer",
        "minimum": 1,
        "maximum": 5
      },
      "nullable": true
    },
    "contents": {
      "type": "string",
      "minLength": 1,
      "maxLength": 500,
      "nullable": true
    },
    "workoutRecords": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "exerciseName": {
            "type": "string"
          },
          "targetMuscles": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "trackingFieldId": {
            "type": "integer"
          },
          "workoutPlanId": {
            "type": "integer"
          },
          "setInfo": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "index": {
                  "type": "integer",
                  "minimum": 1,
                  "maximum": 15
                },
                "reps": {
                  "type": "integer",
                  "minimum": 1,
                  "maximum": 99,
                  "nullable": true
                },
                "weight": {
                  "type": "number",
                  "minimum": 0.1,
                  "maximum": 999,
                  "nullable": true
                },
                "seconds": {
                  "type": "integer",
                  "minimum": 0,
                  "maximum": 10800,
                  "nullable": true
                }
              }
            }
          }
        }
      }
    },
    "scheduleRecords": {
      "type": "object",
      "nullable": true,
      "properties": {
        "heartRates": {
          "type": "array",
          "nullable": true,
          "items": {
            "type": "integer"
          }
        },
        "workoutDuration": {
          "type": "integer",
          "nullable": true
        }
      }
    },
    "threads": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer"
          },        
          "workoutScheduleId": {
            "type": "integer",
            "nullable": true
          },
          "writerType": {
            "type": "string",
            "enum": ["user", "trainer"]
          },
          "type": {
            "type": "string",
            "enum": ["record", "general"]
          },
          "title": {
            "type": "string",
            "nullable": true
          },
          "content": {
            "type": "string"
          },
          "gallery": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "type": {
                  "type": "string",
                  "enum": ["image", "video"]
                },
                "url": {
                  "type": "string"
                },
                "thumbnail": {
                  "type": "string"
                }
              }
            },
            "nullable": true
          },
          "workoutInfo": {
            "type": "object",
            "properties": {
              "workoutScheduleId": {
                "type": "integer"
              },
              "targetMuscleIds": {
                "type": "array",
                "items": {
                  "type": "integer"
                }
              },
              "title": {
                "type": "string"
              },
              "subTitle": {
                "type": "string"
              },
              "workoutDuration": {
                "type": "string"
              },
              "totalSet": {
                "type": "integer"
              },
              "heartRate": {
                "type": "integer"
              },
              "calories": {
                "type": "integer"
              }
            },
            "nullable": true
          },
          "user": {
            "type": "object",
            "properties": {
              "id": {
                "type": "integer"
              },
              "nickname": {
                "type": "string"
              },
              "gender": {
                "type": "string",
                "enum": ["male", "female"]
              }
            },
            "required": [
              "id",
              "nickname",
              "gender"
            ],
            "additionalProperties": false
          },
          "trainer": {
            "type": "object",
            "properties": {
              "id": {
                "type": "integer"
              },
              "nickname": {
                "type": "string"
              },
              "profileImage": {
                "type": "string"
              }
            },
            "required": [
              "id",
              "nickname",
              "profileImage"
            ],
            "additionalProperties": false
          },
          "emojis": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "id": {
                  "type": "integer"
                },
                "emoji": {
                  "type": "string"
                },
                "userId": {
                  "type": "integer",
                  "nullable": true
                },
                "trainerId": {
                  "type": "integer",
                  "nullable": true
                }
              }
            },
            "nullable": true
          },
          "commentCount": {
            "type": "integer"
          },
          "createdAt": {
            "type": "string"
          }
        },
        "required": [
          "id",
          "workoutScheduleId",
          "writerType",
          "type",
          "title",
          "content",
          "gallery",
          "workoutInfo",
          "user",
          "trainer",
          "commentCount",
          "createdAt"
        ],
        "additionalProperties": false
      }
    },
    "additionalProperties": false
  }
}
