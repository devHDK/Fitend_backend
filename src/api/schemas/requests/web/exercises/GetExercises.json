{
  "type": "object",
  "properties": {
    "search": {
      "type": "string"
    },
    "isMe": {
      "type": "boolean"
    },
    "isBookmark": {
      "type": "boolean"
    },
    "tagIds": {
      "type": "array",
      "items": {
        "type": "number"
      }
    },
    "trainerId": {
      "type": "integer"
    },
    "types": {
      "type": "array",
      "items": {
        "type": "string",
        "enum": ["barbell", "dumbbell", "kettlebell", "machine", "bodyweight", "etc"]
      }
    },
    "trackingFieldIds": {
      "type": "array",
      "items": {
        "type": "integer",
        "minimum": 1,
        "maximum": 4
      }
    },
    "targetMuscleIds": {
      "type": "array",
      "items": {
        "type": "integer"
      }
    },
    "start": {
      "type": "integer",
      "minimum": 0,
      "default:": 0
    },
    "perPage": {
      "type": "integer",
      "minimum": 5,
      "maximum": 1000,
      "default:": 25
    }
  },
  "required": ["start", "perPage"],
  "additionalProperties": false
}
